{
    "name": "clevyr/nova-filemanager",
    "description": "Filemanager tool for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "filemanager"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "^4.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../vendor/laravel/nova"
        }
    ],
    "autoload": {
        "psr-4": {
            "Clevyr\\Filemanager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Clevyr\\Filemanager\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
